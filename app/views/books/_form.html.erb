<%= simple_form_for(book) do |f| %>
  <%= f.input :title %>
  <%= f.input :genre %>
  <%= f.input :description %>
  <%= f.input :author %>
  <%= f.input :collection %>
  <%= f.input :editor %>
  <%= f.input :isbn %>
  <%= f.input :state %>
  <%= f.input :wish %>
  <%= f.input :deposit %>
  <%= f.submit %>
<% end %>